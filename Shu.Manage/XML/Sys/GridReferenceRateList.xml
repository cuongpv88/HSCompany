<?xml version="1.0" encoding="utf-8" ?>
<Grid Key="ReferenceRate_ID"
      KeyField=""
      TableName="Sys_ReferenceRate"
      TableView="View_Sys_ReferenceRate"
      SqlOrder="ReferenceRate_IsEnable DESC, ReferenceRate_AdjustDate ASC" 
      SQLField="ReferenceRate_ID ,
                ReferenceRate_InterestRate ,
                ReferenceRate_InterestRatePercent ,
                ReferenceRate_AdjustDate ,
                ReferenceRate_StartDate ,
                ReferenceRate_EndDate ,
                ReferenceRate_IsEnable ,
                ReferenceRate_IsEnableState ,
                ReferenceRate_CreateTime ,
                ReferenceRate_CreateUserId ,
                ReferenceRate_UpdateTime ,
                ReferenceRate_UpdateUserId ,
                ReferenceRate_IsDelete"
      IsShowPagin="true"
