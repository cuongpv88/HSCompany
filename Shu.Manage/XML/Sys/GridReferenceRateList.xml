      ModifyURL="/Sys/ReferenceRateInfo.aspx?flag=edit"
      MenuURL="ReferenceRateList.aspx">
  <Column Key="ReferenceRate_InterestRatePercent" Name="基准利率" width="5"></Column>
  <Column Key="ReferenceRate_AdjustDate" Name="调整日期" width="5"></Column>
  <Column Key="ReferenceRate_StartDate" Name="开始日期" width="5"></Column>
  <Column Key="ReferenceRate_EndDate" Name="结束日期" width="5" ></Column>
  <Column Key="ReferenceRate_IsEnableState" Name="是否启用" width="5" styler="function(value){
